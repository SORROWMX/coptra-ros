[Unit]
Description=Launcher for the ROS master, parameter server and rosout logging node

[Service]
User=orangepi
ExecStart=/bin/sh -c ". /opt/ros/noetic/setup.sh; export ROS_HOSTNAME=localhost; export ROS_IP=127.0.0.1; exec roscore"
Restart=on-failure
RestartSec=3


[Install]
WantedBy=multi-user.target
